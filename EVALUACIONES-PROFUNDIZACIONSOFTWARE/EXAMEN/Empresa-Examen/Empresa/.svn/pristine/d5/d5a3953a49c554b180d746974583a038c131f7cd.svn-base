    private void btnactualizarMouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_btnactualizarMouseClicked
        if (txtnombre2.getText().isEmpty()
                || txtrut2.getText().isEmpty()
                || txtapellido2.getText().isEmpty()
                || txtcodigo2.getText().isEmpty()
                || txtemail2.getText().isEmpty()
                || txtcelular2.getText().isEmpty()
                || txtsueldo2.getText().isEmpty()
                || (!rbncasado2.isSelected() && !rbnsoltero2.isSelected() && !rbnviudo2.isSelected())
                || (cbxdepto2.getSelectedItem().toString().equals("Seleccione"))) {
            JOptionPane.showMessageDialog(this, "Faltan Datos por Ingresar");
            return;
        }
        if ((Integer.parseInt(txtcodigo2.getText()) < 101) && (Integer.parseInt(txtcodigo2.getText()) < 0)) {
            JOptionPane.showMessageDialog(this, "El Código debe ser mayor a cero y menor o igual a 100");
            return;
        }
        if ((Integer.parseInt(txtsueldo2.getText()) < 120000)) {
            JOptionPane.showMessageDialog(this, "El Sueldo debe ser mayor o igual a $120.000");
            return;
        }
        String largocel = txtcelular2.getText();
        if (!(largocel.length() == 9)) {
            JOptionPane.showMessageDialog(this, "El número celular debe contener 9 dígitos");
            return;
        }

        Empleados p = new Empleados();

        p.setRut(txtrut2.getText());
        p.setNombre(txtnombre2.getText());
        p.setApellido(txtapellido2.getText());
        p.setCelular(Integer.parseInt(txtcelular2.getText()));
        p.setEmail(txtemail2.getText());
        p.setSueldo(Integer.parseInt(txtsueldo2.getText()));

        if (rbncasado2.isSelected()) {
            p.setEstado("C");
        }
        if (rbnsoltero2.isSelected()) {
            p.setEstado("S");
        }
        if (rbnviudo2.isSelected()) {
            p.setEstado("V");
        }
        if (cbxdepto2.getSelectedIndex() == 1) {
            p.setDepto("Informática");
        }
        if (cbxdepto2.getSelectedIndex() == 2) {
            p.setDepto("Redes");
        }
        if (cbxdepto2.getSelectedIndex() == 3) {
            p.setDepto("Administración");
        }
        if (cbxdepto2.getSelectedIndex() == 4) {
            p.setDepto("Finanzas");
        }
        if (cbxdepto2.getSelectedIndex() == 5) {
            p.setDepto("Bienestar");
        }
        p.setCodigo(Integer.parseInt(txtcodigo2.getText()));
        Registro g = new Registro();
        JOptionPane.showMessageDialog(this, g.actualizar(p));        // TODO add your handling code here:
    }//GEN-LAST:event_btnactualizarMouseClicked

    private void btneliminarMouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_btneliminarMouseClicked
        if (!btneliminar.isEnabled()) {
            return;
        }
        int codigo = Integer.parseInt(txtcodigo2.getText());
        Registro r = new Registro();
        r.eliminar(codigo);
        JOptionPane.showMessageDialog(this, "Empleado eliminado Correctamente");

        btnactualizar.setEnabled(false);
        btneliminar.setEnabled(false);        // TODO add your handling code here:
    }//GEN-LAST:event_btneliminarMouseClicked
    private void vaciar() {
        txtcodigo2.setText("");
        txtrut2.setText("");
        txtnombre2.setText("");
        txtapellido2.setText("");
        txtcelular2.setText("");
        txtsueldo2.setText("");
        txtemail2.setText("");
        cbxdepto2.setSelectedIndex(0);
        rbncasado2.setSelected(false);
        rbnsoltero2.setSelected(false);
        rbnviudo2.setSelected(false);

    }

    private void verifica(javax.swing.JTextField t) {
        String teclado = t.getText();
        if (teclado.length() > 0) {
            char tecla = teclado.charAt(teclado.length() - 1);
            teclado = teclado.substring(0, teclado.length() - 1);
            if (!Character.isDigit(tecla)) {
                t.setText(teclado);
            }
        }

    }

